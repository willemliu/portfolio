<patTemplate:tmpl name="body">
<?xml version="1.0" encoding="ISO-8859-1"?>
<xml>
  <fileName>{FILE_NAME}</fileName>
  <item>
    <html>
      &lt;div CLASS="WINDOW_BAR"&gt;
        &lt;div class="WINDOW_TITLE"&gt;
          Add/Edit Sound Mixes
        &lt;/div&gt;
        &lt;a onClick="var fade = new MyFade(); fade.FadeOut('ADD_SOUND_MIX',0, 1, 'fade'); postData('window_close.php', '', 'window=ADD_SOUND_MIX');" onMouseDown="mouse.dispensemove();"&gt;
          Close
        &lt;/a&gt;
      &lt;/div&gt;
    </html>
  </item>
  <item>
    <html>
      Movie: {FILM_TITLE}
    </html>
  </item>
  <item>
    <html>
      &lt;br&gt;
      &lt;br&gt;
    </html>
  </item>
  <item>
    <html>
      Sound Mix:
      &lt;br&gt;
    </html>
  </item>
  <item>
    <html>
      &lt;table&gt;
        &lt;tr&gt;
    </html>
  </item>
  <item>
    <html>
      &lt;td&gt;
      &lt;select id="add_sound_mix_id[]" name="add_sound_mix_id[]" multiple="multiple" size="20" onDblClick="MoveListItem('add_sound_mix_id[]', 'selectable_sound_mix');" onMouseDown="mouse.dispensemove();"&gt;
    </html>
  </item>
  <patTemplate:tmpl name="show_selected_sound_mix" type="SimpleCondition" requiredVars="CONDITION">
  <patTemplate:tmpl name="selected_sound_mix">
  <item>
    <html>
      &lt;option value="{SOUND_MIX_ID}" {SELECTED}&gt;{SOUND_MIX}&lt;/option&gt;
    </html>
  </item>
  </patTemplate:tmpl>
  </patTemplate:tmpl>
  <item>
    <html>
      &lt;/select&gt;
      &lt;/td&gt;
    </html>
  </item>
  <item>
    <html>
      &lt;td&gt;
      &lt;input type="button" name="Add" value="&lt;&lt; Add" style="width: 100px; text-align: right;" onClick="MoveListItem('selectable_sound_mix', 'add_sound_mix_id[]');" onMouseDown="mouse.dispensemove();"&gt;
      &lt;br&gt;
      &lt;input type="button" name="Remove" value="Remove &gt;&gt;" style="width: 100px; text-align: left;" onClick="MoveListItem('add_sound_mix_id[]', 'selectable_sound_mix');" onMouseDown="mouse.dispensemove();"&gt;
      &lt;/td&gt;
    </html>
  </item>
  <item>
    <html>
      &lt;td&gt;
      &lt;select id="selectable_sound_mix" name="selectable_sound_mix" multiple="multiple" size="20" onDblClick="MoveListItem('selectable_sound_mix', 'add_sound_mix_id[]');" onMouseDown="mouse.dispensemove();"&gt;
    </html>
  </item>
  <patTemplate:tmpl name="show_sound_mix" type="SimpleCondition" requiredVars="CONDITION">
  <patTemplate:tmpl name="sound_mix">
  <item>
    <html>
      &lt;option value="{SOUND_MIX_ID}" {SELECTED}&gt;{SOUND_MIX}&lt;/option&gt;
    </html>
  </item>
  </patTemplate:tmpl>
  </patTemplate:tmpl>
  <item>
    <html>
      &lt;/select&gt;
      &lt;/td&gt;
    </html>
  </item>
  <item>
    <html>
        &lt;/tr&gt;
      &lt;/table&gt;
    </html>
  </item>
  <item>
    <html>
      &lt;br&gt;
      &lt;br&gt;
    </html>
  </item>
  <item>
    <html>
      &lt;input type="button" id="add_sound_mix_add" name="add_sound_mix_add" value="Update" onClick="{UPDATE_COMMAND}" onMouseDown="mouse.dispensemove();"&gt;
    </html>
  </item>
  <patTemplate:tmpl name="setTimeout">
  <setTimeout>
    <Command>{COMMAND}</Command>
    <TimeOut>
      {MILLISECONDS}
    </TimeOut>
  </setTimeout>
  </patTemplate:tmpl>
</xml>
</patTemplate:tmpl>