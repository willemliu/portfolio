<patTemplate:tmpl name="body">
<?xml version="1.0" encoding="ISO-8859-1"?>
<xml>
  <fileName>{FILE_NAME}</fileName>
  <item>
    <html>
      &lt;div CLASS="WINDOW_BAR"&gt;
        &lt;div class="WINDOW_TITLE"&gt;
          Add Cast
        &lt;/div&gt;
        &lt;a onClick="var fade = new MyFade(); fade.FadeOut('ADD_CAST',0, 1, 'fade'); postData('window_close.php', '', 'window=ADD_CAST');" onMouseDown="mouse.dispensemove();"&gt;
          Close
        &lt;/a&gt;
      &lt;/div&gt;
    </html>
  </item>
  <item>
    <html>
      Movie: {FILM_TITLE}
    </html>
  </item>
  <item>
    <html>
      &lt;br&gt;
      Search: &lt;input type="text" id="cast_search_filter" name="cast_search_filter" value="{SEARCH_FILTER}" size="30" onMouseDown="mouse.dispensemove();" onKeyUp="if(KeyCheck(event, 13)) postData('search_cast.php', 'SEARCH_CAST', 'film_id=' + escape(encodeURI({FILM_ID})) + '&amp;search_filter=' + escape(encodeURI(document.getElementById('cast_search_filter').value)));" onKeyDown="document.getElementById('add_cast_add').disabled = true;"&gt;
    </html>
  </item>
  <item>
    <html>
      &lt;input type="button" id="add_cast_search" name="add_cast_search" value="Search" onMouseDown="mouse.dispensemove();" onClick="postData('search_cast.php', 'SEARCH_CAST', 'film_id=' + escape(encodeURI({FILM_ID})) + '&amp;search_filter=' + escape(encodeURI(document.getElementById('cast_search_filter').value)));"&gt;
      &lt;input type="button" id="add_cast_add" name="add_cast_add" value="Add" onMouseDown="mouse.dispensemove();" onClick="postData('add_cast_added.php', 'INFORMATION', 'film_id=' + escape(encodeURI({FILM_ID})) + '&amp;search_filter=' + escape(encodeURI(document.getElementById('cast_search_filter').value)));" disabled&gt;
      &lt;br&gt;
      &lt;br&gt;
    </html>
  </item>
  <item>
    <html>
      &lt;div id="SEARCH_CAST"&gt;&lt;/div&gt;
    </html>
  </item>
  <patTemplate:tmpl name="setTimeout">
  <setTimeout>
    <Command>{COMMAND}</Command>
    <TimeOut>
      {MILLISECONDS}
    </TimeOut>
  </setTimeout>
  </patTemplate:tmpl>
  <performance>
    <parseTime>
      {PARSE_TIME}
    </parseTime>
    <dbqueries>
      {DB_QUERIES}
    </dbqueries>
  </performance>
</xml>
</patTemplate:tmpl>