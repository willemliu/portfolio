<patTemplate:tmpl name="body">
<?xml version="1.0" encoding="ISO-8859-1"?>
<xml>
  <fileName>{FILE_NAME}</fileName>
  <item>
    <html>
      &lt;div CLASS="WINDOW_BAR"&gt;
        &lt;div class="WINDOW_TITLE"&gt;
          View Trailer
        &lt;/div&gt;
        &lt;a onClick="var fade = new MyFade(); fade.FadeOut('VIEW_TRAILER',0, 1, 'fade'); postData('window_close.php', '', 'window=VIEW_TRAILER');" onMouseDown="mouse.dispensemove();"&gt;
          Close
        &lt;/a&gt;
      &lt;/div&gt;
    </html>
  </item>
  <item>
    <html>
      Film: {FILM}
      &lt;br&gt;
      Title: {TITLE}
      &lt;br&gt;
      Submitted by: {NICK_NAME}
      &lt;br&gt;
    </html>
  </item>
  <item>
    <html>
      &lt;a onClick="postData('remove_trailer.php', 'INFORMATION', 'trailer_id={ID}');" onMouseDown="mouse.dispensemove();"&gt;[Delete]&lt;/a&gt;
      &lt;a onClick="alert('Feature not available yet.');" onMouseDown="mouse.dispensemove();"&gt;[Report]&lt;/a&gt;
      &lt;br&gt;
    </html>
  </item>
  <item>
    <html>
      &lt;div onMouseDown="mouse.dispensemove();"&gt;
      {TRAILER}
      &lt;/div&gt;
    </html>
  </item>
  <patTemplate:tmpl name="setTimeout">
  <setTimeout>
    <Command>{COMMAND}</Command>
    <TimeOut>
      {MILLISECONDS}
    </TimeOut>
  </setTimeout>
  </patTemplate:tmpl>
</xml>
</patTemplate:tmpl>