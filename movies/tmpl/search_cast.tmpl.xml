<patTemplate:tmpl name="body">
<?xml version="1.0" encoding="ISO-8859-1"?>
<xml>
  <fileName>{FILE_NAME}</fileName>
  <patTemplate:tmpl name="show_rows" type="SimpleCondition" requiredVars="CONDITION">
  <patTemplate:tmpl name="rows">
  <item>
    <html>
        &lt;a onMouseDown="mouse.dispensemove();" onClick="postData('add_cast_added.php', 'INFORMATION', 'film_id={FILM_ID}&amp;cast_id={ID}');" title="Click to Add"&gt;{NAME}&lt;/a&gt;&lt;br&gt;
    </html>
  </item>
  </patTemplate:tmpl>
  </patTemplate:tmpl>
  <patTemplate:tmpl name="setTimeout">
  <setTimeout>
    <Command>{COMMAND}</Command>
    <TimeOut>
      {MILLISECONDS}
    </TimeOut>
  </setTimeout>
  </patTemplate:tmpl>
  <performance>
    <parseTime>
      {PARSE_TIME}
    </parseTime>
    <dbqueries>
      {DB_QUERIES}
    </dbqueries>
  </performance>
</xml>
</patTemplate:tmpl>