<patTemplate:tmpl name="body">
<?xml version="1.0" encoding="ISO-8859-1"?>
<xml>
  <fileName>{FILE_NAME}</fileName>
  <item>
    <html>
      &lt;div CLASS="WINDOW_BAR"&gt;
        &lt;div class="WINDOW_TITLE"&gt;
          Menu
        &lt;/div&gt;
      &lt;/div&gt;
    </html>
  </item>
  <patTemplate:tmpl name="link">
  <item>
    <html>
      &lt;a onclick="{LINK_COMMAND}" onMouseDown="mouse.dispensemove();"&gt;{LINK_TEXT}&lt;/a&gt;
      &lt;br&gt;
    </html>
  </item>
  </patTemplate:tmpl>
  <patTemplate:tmpl name="setTimeout">
  <setTimeout>
    <Command>{COMMAND}</Command>
    <TimeOut>
      {MILLISECONDS}
    </TimeOut>
  </setTimeout>
  </patTemplate:tmpl>
</xml>
</patTemplate:tmpl>